    "miniflare": "^2.0.0-rc.3",
    "openapi-typescript": "^4.0.2",
    "playwright-test": "^7.2.1",
    "process": "^0.11.10",
    "readable-stream": "^3.6.0",
    "sade": "^1.7.4"
  }
