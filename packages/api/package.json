    "merge-options": "^3.0.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.7.1",
    "@sentry/cli": "^1.71.0",
    "@sentry/webpack-plugin": "^1.16.0",
    "@types/assert": "^1.5.6",
    "@types/git-rev-sync": "^2.0.0",
    "@types/mocha": "^9.0.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "delay": "^5.0.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.13",
